    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "fuse.js": "^7.1.0",
    "multer": "^2.0.2",
    "pg": "^8.13.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^10.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.11.10",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.0.1",
    "tsx": "^4.19.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
